{
  "name": "stedsbookings",
  "productName": "St.EdwardsBookings",
  "version": "1.4.9",
  "description": "Electron Application for St. Edwards Fellwalkers booking system",
  "main": "app/app.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "couchapp": "couchapp push src/designdocs/members.js http://aidan:Fw57kUbxCAjH@nicholware.com:5984/bookings",
    "couchapp:dev": "couchapp push src/designdocs/members.js http://aidan:admin@localhost:5984/devbookings",
    "pack": "build --win --ia32",
    "dist": "build --win --ia32",
    "predist": "npm-run-all -l -s build:copyfiles build:html build:package build:js build:css ",
    "release": "build",
    "debug": "DEBUG=steds:* electron .",
    "start": "cross-env NODE_ENV=production electron ./",
    "deploy": "gulp build && dist",
    "build:js": "babel --presets='env,stage-0,react' --plugins=transform-decorators-legacy -d app/ src/",
    "build:app": "babel --only=app.js --presets='es2015,stage-0,react' -d app/ src/",
    "rollup": "BABEL_ENV=rollup rollup -c",
    "build:css": "postcss src/styles/**/*.css --dir=./app/styles",
    "prod:html": "babel-node --presets=es2015 scripts/buildHtml.js --src=src/*html --dest=app --css=src/styles/*.css --js=../app/bundle.js",
    "build:html": "babel-node --presets=es2015,stage-0 scripts/buildHtml.js --src=src/*html --dest=app --css=src/styles/*.css --js=../app/index.js",
    "build:copyfiles": "copyfiles assets/*.* fonts/**/*.*  app & copyfiles -f src/windows/*.html app/windows",
    "build:package": "babel-node scripts/buildPackage --dest=app",
    "refresh:dev": "babel-node --no-babelrc --presets=es2015,stage-0 ./refreshDevFromLiveNoReformat",
    "reload:dev": "node ./dumpLiveRestoreDev",
    "build:conflicts": "pkg ./checkConflicts/checkForWalkDocConflictspkg --target=macos --out-path=./checkConflicts/checkForWalkDocConflicts",
    "build:dist": "run-s -l build:app rollup build:css build:copyfiles prod:html build:package",
    "my:build": "npm run build:dist && electron app/app.js",
    "dev:js": "babel -d .serve/ src/ --watch",
    "dev:css": "npm run build:css -- --dir=.serve/styles --watch",
    "watch:js": "npm run build:js -- --watch",
    "watch:css": "npm run build:css --  --watch",
    "rollup:js": "BABEL_ENV=rollup rollup -w -c config/dev.js",
    "dev:copyfiles": "copyfiles assets fonts .serv",
    "watch:dist": "run-s -l watch:js watch:css copy:assets copy:fonts build:html build:package",
    "run:dist": "npm run build:dist && electron app/app.js",
    "open:dev": "electron app/app.js",
    "display:logs": "cd ~/Documents/StEdwards/logs && bunyan bookings.log -l warn",
    "watch:build": "watch 'npm run build:dist' .",
    "dev": "npm-run-all -l -s build:copyfiles build:html build:package -p watch:js watch:css ",
    "devm": "npm-run-all -p -l dev open:dev",
    "test": "jest"
  },
  "build": {
    "appId": "co.uk.stedwardsfellwalkers.bookings",
    "asar": true,
    "asarUnpack": "./mobx/memberSt*",
    "win": {
      "icon": "./assets/st_edwardslogo.ico",
      "certificateFile": "../SelfSignedCert.pfx",
      "certificatePassword": "a1dan!",
      "target": "NSIS",
      "publish": [
        "github"
      ]
    }
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*..js",
      "!dist/**/*"
    ],
    "verbose": true,
    "concurrency": 5,
    "require": [
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "views": "components/views",
            "containers": "components/containers"
          }
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AidanNichol/StEdsElectron.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node"
  ],
  "author": "Aidan Nichol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AidanNichol/StEdsElectron/issues"
  },
  "homepage": "https://github.com/AidanNichol/StEdsElectron",
  "dependencies": {
    "@blueprintjs/core": "^1.1.0",
    "@fortawesome/fontawesome-pro": "^5.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/pro-light-svg-icons": "^5.5.0",
    "@fortawesome/pro-regular-svg-icons": "^5.5.0",
    "@fortawesome/pro-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "argv": "0.0.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "1.7",
    "babel-runtime": "^6.11.6",
