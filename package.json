    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "classnames": "^2.2.6",
    "colors": "^1.3.2",
    "conf": "^1.4.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "couchapp": "^0.11.0",
    "debug": "^3.1.0",
    "electron-debug": "^1.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-settings": "^3.1.3",
    "electron-store": "^1.3.0",
    "electron-version": "^1.0.2",
    "emittery": "^0.4.1",
    "fs-jetpack": "^2.2.0",
    "getmac": "^1.4.6",
    "global": "^4.3.2",
    "hint.css": "^2.4.0",
    "lodash": "^4.17.11",
    "madge": "^3.1.1",
    "mailgun-js": "^0.22.0",
    "minimist": "^1.2.0",
    "mobx": "^5.6.0",
    "mobx-react": "^5.3.6",
    "mobx-react-devtools": "^6.0.3",
    "mobx-utils": "^5.0.4",
    "nconf": "^0.10.0",
    "node-notifier": "^5.3.0",
    "open": "0.0.5",
    "pdfkit": "^0.8.0",
    "pouchdb": "^6.3.4",
    "pouchdb-authentication": "^0.5.4",
    "pouchdb-dump-cli": "^2.0.6",
    "pouchdb-seed-design": "^0.3.0",
    "pretty-format": "^23.6.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "16.6.3",
