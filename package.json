{
  "name": "stedsbookings",
  "productName": "St.EdwardsBookings",
  "version": "0.2.1",
  "description": "Electron Application for St. Edwards Fellwalkers booking system",
  "main": "app/app.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "pack": "build --win --ia32",
    "dist": "build --win --ia32",
    "release": "build",
    "debug": "DEBUG=steds:* electron .",
    "start": "cross-env NODE_ENV=production electron ./",
    "deploy": "gulp build && dist",
    "build:js": "babel -d app/ src/",
    "build:app": "babel --only=app.js --presets=es2015,stage-0,react -d app/ src/",
    "rollup": "BABEL_ENV=rollup rollup -c",
    "build:css": "postcss -c postcss.json ",
    "prod:html": "babel-node --presets=es2015 scripts/buildHtml.js --src=src/*html --dest=app --css=src/styles/*.css --js=../app/bundle.js",
    "build:html": "babel-node --presets=es2015,stage-0 scripts/buildHtml.js --src=src/*html --dest=app --css=src/styles/*.css --js=../app/index.js",
    "build:copyfiles": "copyfiles assets fonts app",
    "build:package": "babel-node scripts/buildPackage --dest=app",
    "build:dist": "run-s -l build:app rollup build:css build:copyfiles prod:html build:package",
    "my:build": "npm run build:dist && electron app/app.js",
    "dev:js": "babel -d .serve/ src/ --watch",
    "dev:css": "npm run build:css -- --dir=.serve/styles --watch",
    "watch:js": "npm run build:js -- --watch",
    "watch:css": "npm run build:css -- --dir=app/styles --watch",
    "rollup:js": "BABEL_ENV=rollup rollup -w -c config/dev.js",
    "dev:copyfiles": "copyfiles assets fonts .serv",
    "watch:dist": "run-s -l watch:js watch:css copy:assets copy:fonts build:html build:package",
    "run:dist": "npm run build:dist && electron app/app.js",
    "open:dev": "electron app/app.js",
    "watch:build": "watch 'npm run build:dist' .",
    "dev": "npm-run-all -l -s build:copyfiles build:html build:package -p watch:js watch:css ",
    "devm": "npm-run-all -p -l dev open:dev",
    "test": "NODE_PATH=src ava"
  },
  "build": {
    "appId": "co.uk.stedwardsfellwalkers.bookings",
    "win": {
      "iconUrl": "http://stedwardsfellwalkers.co.uk/favicon.ico",
      "remoteReleases": "https://github.com/AidanNichol/StEdsElectron",
      "certificateFile": "../SelfSignedCert.pfx",
      "certificatePassword": "a1dan!",
      "target": "NSIS",
      "publish": [
        "github"
      ]
    }
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*..js",
      "!dist/**/*"
    ],
    "verbose": true,
    "concurrency": 5,
    "require": [
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "views": "components/views",
            "containers": "components/containers"
          }
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AidanNichol/StEdsElectron.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node"
  ],
  "author": "Aidan Nichol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AidanNichol/StEdsElectron/issues"
  },
  "homepage": "https://github.com/AidanNichol/StEdsElectron",
  "dependencies": {
    "argv": "0.0.2",
    "babel-polyfill": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "debounce": "^1.0.0",
    "debug": "^2.2.0",
    "electron-debug": "^1.0.1",
    "electron-devtools-installer": "^2.0.1",
    "electron-settings": "^2.2.2",
    "electron-version": "^1.0.2",
    "glob-fs": "^0.1.6",
    "hint.css": "^2.4.0",
    "icepick": "^1.3.0",
    "is-online": "^5.1.2",
    "open": "0.0.5",
    "pdfkit": "^0.8.0",
    "pouchdb": "^5.4.5",
    "pouchdb-auth": "^2.1.1",
    "pouchdb-authentication": "^0.5.4",
    "pouchdb-seed-design": "^0.3.0",
    "ramda": "^0.22.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-input-autosize": "^1.1.0",
    "react-json-tree": "^0.10.0",
    "react-object-editor": "^0.1.2",
    "react-redux": "^4.4.5",
    "react-select": "^1.0.0-beta14",
    "react-textarea-autosize": "^4.0.4",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-form": "^6.0.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.12.1",
    "reselect": "^2.5.1",
    "xdate": "^0.8.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "browserify": "^13.1.0",
    "copyfiles": "^1.0.0",
    "cross-env": "^3.1.2",
    "cssnano": "^3.8.0",
    "del": "^2.2.2",
    "devtron": "^1.4.0",
    "electron": "^1.4.3",
    "electron-builder": "^8.5.3",
    "electron-connect": "^0.6.0",
    "electron-packager": "^8.1.0",
    "eslint": "^3.10.0",
    "eslint-import-resolver-babel-module": "^2.2.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-no-unused-vars-rest": "^1.0.3",
    "eslint-plugin-react": "^6.3.0",
    "fs-jetpack": "^0.10.2",
    "glob": "^7.1.1",
    "ignore-styles": "^5.0.1",
    "json-loader": "^0.5.4",
    "main-bower-files": "^2.13.1",
    "npm-run-all": "^3.1.1",
    "postcss-cli": "^2.6.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.2.0",
    "postcss-inline-comment": "^3.0.0",
    "postcss-nested": "^1.0.0",
    "precss": "^1.4.0",
    "rimraf": "^2.5.4",
    "svgstore": "^1.1.0",
    "timekeeper": "^1.0.0",
    "watch": "^1.0.1"
  }
}
