    "enzyme": "^3.1.0",
    "eslint": "^5.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-svg-path": "^2.1.0",
