{
  "name": "stedsbookings",
  "productName": "St.EdwardsBookings",
  "version": "1.5.0",
  "description": "Electron Application for St. Edwards Fellwalkers booking system",
  "main": "app/app.js",
  "license": "MIT",
  "scripts": {
    "devm": "npm-run-all -p -l process open:dev",
    "reload:dev": "node ./dumpLiveRestoreDev",
    "dist": "build --win --ia32",
    "postinstall": "electron-builder install-app-deps",
    "couchapp": "couchapp push src/designdocs/members.js http://aidan:Fw57kUbxCAjH@nicholware.com:5984/bookings",
    "couchapp:dev": "couchapp push src/designdocs/members.js http://aidan:admin@localhost:5984/devbookings",
    "predist": "npm-run-all -l -s process:copyfiles process:html process:package process:js process:css ",
    "process": "npm-run-all -l -s process:copyfiles process:html process:package -p watch:js watch:css ",
    "process:copyfiles": "copyfiles assets/*.* fonts/**/*.*  app & copyfiles -f src/windows/*.html app/windows",
    "process:html": "node scripts/buildHtml.js --src=src/*html --dest=app --css=src/styles/*.css",
    "process:package": "node scripts/buildPackage --dest=app",
    "watch:js": "npm run process:js -- --watch",
    "watch:css": "npm run process:css --  --watch ",
    "process:js": "babel  -d app/ src/ --verbose",
    "process:css": "postcss src/styles/**/*.css --dir=./app/styles --verbose",
    "open:dev": "electron app/app.js",
    "scripts-tree": "npm-scripts-tree -p ",
    "scripts-tree:print": "npm-scripts-tree -p | lp -o orientation-requested=5 -o fit-to-page -o sides=one-sided -",
    "junk": "-----------------------------------------------------------------------------------------------------------",
    "junk:prod:html": "babel-node --presets=es2015 scripts/buildHtml.js --src=src/*html --dest=app --css=src/styles/*.css --js=../app/bundle.js",
    "junk:refresh:dev": "babel-node --no-babelrc --presets=es2015,stage-0 ./refreshDevFromLiveNoReformat",
    "junk:dev:copyfiles": "copyfiles assets fonts .serv",
    "junk:dev:js": "babel -d .serve/ src/ --watch",
    "junk:dev:css": "npm run process:css -- --dir=.serve/styles --watch",
    "junk:watch:dist": "run-s -l watch:js watch:css copy:assets copy:fonts process:html process:package",
    "junk:display:logs": "cd ~/Documents/StEdwards/logs && bunyan bookings.log -l warn",
    "junk:pack": "build --win --ia32",
    "junk:release": "build",
    "junk:debug": "DEBUG=steds:* electron .",
    "junk:start": "cross-env NODE_ENV=production electron ./",
    "junk:process:conflicts": "pkg ./checkConflicts/checkForWalkDocConflictspkg --target=macos --out-path=./checkConflicts/checkForWalkDocConflicts",
    "Rollup?": "----------------------------------------------------------------------------------",
    "Rollup?:rollup:js": "BABEL_ENV=rollup rollup -w -c config/dev.js",
    "Rollup?:rollup": "BABEL_ENV=rollup rollup -c",
    "Rollup?:process:dist": "run-s -l process:app rollup process:css process:copyfiles prod:html process:package",
    "Rollup?:process:app": "babel --only=app.js --presets='es2015,stage-0,react' -d app/ src/",
    "Rollup?:run:dist": "npm run process:dist && electron app/app.js",
    "Rollup?:watch:build": "watch 'npm run process:dist' .",
    "Rollup?:my:build": "npm run process:dist && electron app/app.js",
    "Rollup?:deploy": "gulp build && dist",
    "test": "jest"
  },
  "build": {
    "appId": "co.uk.stedwardsfellwalkers.bookings",
    "asar": true,
    "asarUnpack": "./mobx/memberSt*",
    "win": {
      "icon": "./assets/st_edwardslogo.ico",
      "certificateFile": "../SelfSignedCert.pfx",
      "certificatePassword": "a1dan!",
      "target": "NSIS",
      "publish": [
        "github"
      ]
    }
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*..js",
      "!dist/**/*"
    ],
    "verbose": true,
    "concurrency": 5,
    "require": [
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AidanNichol/StEdsElectron.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node"
  ],
  "author": "Aidan Nichol",
  "bugs": {
    "url": "https://github.com/AidanNichol/StEdsElectron/issues"
  },
  "homepage": "https://github.com/AidanNichol/StEdsElectron",
  "dependencies": {
