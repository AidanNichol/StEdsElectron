{
  "name": "StEdsBookings",
  "version": "0.1.0",
  "description": "Electron Application for St. Edwards Fellwalkers booking system",
  "main": "src/app.js",
  "scripts": {
    "build": "electron-packager-compile . --out=dist  --app-version=$npm_package_version --version=1.4.3 --asar --prune --overwrite --platform=darwin --arch=x64",
    "debug": "DEBUG=steds:* electron .",
    "start": "cross-env ENV=development electron .",
    "dev": "./node_modules/.bin/electron .",
    "test": "NODE_PATH=src ava",
    "startx": "rm -rf .cache && cross-env NODE_ENV=development electron .",
    "clean-cache": "rm -rf .cache",
    "clean-source": "node cleaner.js \"./dist\"",
    "compile": "cross-env NODE_ENV=production electron-compile --appDir . ./src",
    "package": "run-s clean-cache compile package-linux package-win clean-source",
    "package-linux": "cross-env NODE_ENV=production electron-packager . app --out=dist --platform=linux --arch=all --overwrite --ignore=\".compilerc|.gitignore|cleaner.js\"",
    "package-win": "cross-env NODE_ENV=production electron-packager . app --out=dist --platform=win32 --arch=all --overwrite --ignore=\".compilerc|.gitignore|cleaner.js\"",
    "quick-package": "run-s clean-cache compile package-for-this clean-source",
    "package-for-this": "cross-env NODE_ENV=production electron-packager . app --out=dist --overwrite --ignore=\".compilerc|.gitignore|cleaner.js\""
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*..jsx}",
      "!dist/**/*"
    ],
    "verbose": true,
    "concurrency": 5,
    "require": [
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AidanNichol/StEdsElectron.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node"
  ],
  "author": "Aidan Nichol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AidanNichol/StEdsElectron/issues"
  },
  "homepage": "https://github.com/AidanNichol/StEdsElectron",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "debounce": "^1.0.0",
    "debug": "^2.2.0",
    "electron-compile": "^3.3.6",
    "electron-devtools-installer": "^2.0.1",
    "hint.css": "^2.4.0",
    "icepick": "^1.3.0",
    "pdfmake": "^0.1.18",
    "pouchdb": "^5.4.5",
    "pouchdb-auth": "^2.1.1",
    "pouchdb-authentication": "^0.5.4",
    "pouchdb-seamless-auth": "^1.0.4",
    "pouchdb-seed-design": "^0.3.0",
    "precss": "^1.4.0",
    "ramda": "^0.22.1",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.3.2",
    "react-input-autosize": "^1.1.0",
    "react-redux": "^4.4.5",
    "react-select": "^1.0.0-beta14",
    "react-textarea-autosize": "^4.0.4",
    "rebass": "^0.3.1",
    "redux": "^3.5.2",
    "redux-act": "^0.5.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-form": "^6.0.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.11.1",
    "reselect": "^2.5.1",
    "underscore.string": "^3.2.2",
    "xdate": "^0.8.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016-node5": "^1.1.2",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^2.2.0",
    "electron": "^1.4.3",
    "electron-compilers": "^4.0.0",
    "electron-connect": "^0.6.0",
    "electron-packager": "^8.1.0",
    "electron-prebuilt-compile": "^1.4.2",
    "eslint": "^3.7.1",
    "eslint-plugin-no-unused-vars-rest": "^1.0.3",
    "eslint-plugin-react": "^6.3.0",
    "ignore-styles": "^5.0.1",
    "svgstore": "^1.1.0",
    "timekeeper": "^0.1.1"
  }
}
