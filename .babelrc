{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["chrome >= 59"],
        "node": "8.2"
      }
    }],
    "react"
  ],

  "plugins": [
    "transform-decorators-legacy",
    "lodash", [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "views": "components/views"
        }
      }
    ]
  ]
}
