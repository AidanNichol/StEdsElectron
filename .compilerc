{
  "env": {
    "development": {
      "application/javascript": {
        "presets": ["es2015", "stage-0", "react"],
        "plugins": ["transform-class-properties"],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": ["es2015", "stage-0", "react"],
        "plugins": ["transform-class-properties"],
        "sourceMaps": false
      }
    }
  }
}
